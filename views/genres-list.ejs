<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Add genre</title>
        <link rel="stylesheet" href="/index.css">
    </head>
    <body>
	  	
        <%- include('_side-bar') %>
        <%- include('_top-bar') %>
	
        <main>
			
            <div class="dashboard">
                <div class="named-list">
                    <h2>Genres</h2>
                    <ul>

                        <% genreList.forEach((genre) => { %>
                        <%- include('_genre-item', {genre}) %>
                        <% }) %>
                    </ul>
                </div>
            </div>
        </main>
    </body>
</html>
