<div class="item-wrapper">
    <%- include('_item-actions', {itemType: 'games', itemId: game.id}) %>
    <div class="item game-item">
        <img src="<%=game.b64%>" alt="<%=game.game_name%>">
        <h3><%=game.game_name%></h3>
        <p class="publisher"><%=game.publisher_name%></p>
        <p class="genres"><%=game.genres%></p>
    </div>
</div>
